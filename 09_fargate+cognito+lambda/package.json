{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=\"local\" nodemon -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node src/index.js",
    "lint": "eslint --fix --ext \".ts\" \"**/*.ts\"",
    "typeorm:migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migration/create-table -d ./src/data-source.ts",
    "typeorm:migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
    "deploy:pipeline:it": "cdk deploy tanso-pipeline-it --require-approval never -c env=it",
    "destroy:pipeline:it": "cdk destroy tanso-pipeline-it --require-approval never -c env=it",
    "deploy:pipeline:st": "cdk deploy tanso-pipeline-st --require-approval never -c env=st",
    "destroy:pipeline:st": "cdk destroy tanso-pipeline-st --require-approval never -c env=st"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.533.0",
    "@aws-sdk/client-dynamodb": "^3.533.0",
    "@aws-sdk/client-secrets-manager": "^3.535.0",
    "@aws-sdk/lib-dynamodb": "^3.533.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/routes": "^5.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "aws-cdk": "2.131.0",
    "aws-cdk-lib": "2.131.0",
    "constructs": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "json-schema-to-ts": "^3.0.0",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
